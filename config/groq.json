{
  "agents": {
    "defaults": {
      "model": {
        "primary": "groq/llama-3.3-70b-versatile"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "models": {
    "providers": {
      "groq": {
        "baseUrl": "https://api.groq.com/openai/v1",
        "models": [
          {
            "id": "llama-3.3-70b-versatile"
          },
          {
            "id": "llama-3.1-8b-instant"
          },
          {
            "id": "mixtral-8x7b-32768"
          }
        ]
      }
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "trustedProxies": [
      "172.16.0.0/12",
      "10.0.0.0/8",
      "192.168.0.0/16"
    ],
    "controlUi": {
      "enabled": true,
      "allowInsecureAuth": true
    }
  },
  "browser": {
    "headless": true,
    "defaultProfile": "openclaw",
    "noSandbox": true
  }
}
